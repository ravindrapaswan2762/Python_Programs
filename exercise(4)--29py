# QUESTION :- solved


num1 = int(input("enter first number : "))
num2 = int(input("enter second number : "))
if (num1 is num2):
    print("True\n")
    print("*\n")
    print("* *\n")
    print("* * *\n")
    print("* * * *\n")

else:
    print("False\n")
    print("* * * *\n")
    print("* * * \n")
    print("* * \n")
    print("* \n")



